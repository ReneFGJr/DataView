{
    "displayName": "Brapci DataView - IMAGENS",
    "description": "Visualizado de dados Imagens PNG",
    "toolName": "brapci_dataview",
    "scope": "file",
    "types": [
        "preview"
    ],
    "toolUrl": "$HTTP/index.php/dataview/view/png",
    "contentType": "image/png",
    "toolParameters": {
        "queryParameters": [
            {
                "fileid": "{fileId}"
            },
            {
                "siteUrl": "{siteUrl}"
            },
            {
                "PID": "{datasetPid}"
            },
            {
                "key": "{apiToken}"
            },
            {
                "datasetId": "{datasetId}"
            },
            {
                "localeCode": "{localeCode}"
            }
        ]
    }
}