"displayName": "Brapci DataView - Dataverse",
"description": "Visualizado de dados",
"toolName": "brapci_dataview",
"scope": "file",
"types": [
    "preview"
],
"toolUrl": "$HTTP",
"contentType": "text/tab-separated-values",
"toolParameters": {
    "queryParameters": [
        {
            "fileid": "{fileId}"
        },
        {
            "siteUrl": "{siteUrl}"
        },
        {
            "PID": "{datasetPid}"
        },
        {
            "key": "{apiToken}"
        },
        {
            "datasetId": "{datasetId}"
        },
        {
            "localeCode": "{localeCode}"
        }
    ]
}
}