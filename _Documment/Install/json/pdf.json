{
    "displayName": "Brapci DataView - Dataverse - PDF",
    "description": "Visualizado de dados PDF",
    "toolName": "brapci_dataview",
    "scope": "file",
    "types": [
        "preview"
    ],
    "toolUrl": "$HTTP",
    "contentType": "application/pdf",
    "toolParameters": {
        "queryParameters": [
            {
                "fileid": "{fileId}"
            },
            {
                "siteUrl": "{siteUrl}"
            },
            {
                "PID": "{datasetPid}"
            },
            {
                "key": "{apiToken}"
            },
            {
                "datasetId": "{datasetId}"
            },
            {
                "localeCode": "{localeCode}"
            }
        ]
    }
}