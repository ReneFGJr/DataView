{
"displayName": "Brapci DataView - TXT",
"description": "Visualizado de arquivo texto",
"toolName": "brapci_dataview",
"scope": "file",
"types": [
    "preview"
],
"toolUrl": "$HTTP/dataview/index.php/dataview/view/txt",
"contentType": "text/plain",
"toolParameters": {
    "queryParameters": [
        {
            "fileid": "{fileId}"
        },
        {
            "siteUrl": "{siteUrl}"
        },
        {
            "PID": "{datasetPid}"
        },
        {
            "key": "{apiToken}"
        },
        {
            "datasetId": "{datasetId}"
        },
        {
            "localeCode": "{localeCode}"
        }
    ]
}
}