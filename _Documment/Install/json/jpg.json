{
    "displayName": "Brapci DataView - IMAGENS",
    "description": "Visualizado de dados Imagens JPG",
    "toolName": "brapci_dataview",
    "scope": "file",
    "types": [
        "preview"
    ],
    "toolUrl": "$HTTP/dataview/index.php/dataview/view/jpg",
    "contentType": "image/jpeg",
    "toolParameters": {
        "queryParameters": [
            {
                "fileid": "{fileId}"
            },
            {
                "siteUrl": "{siteUrl}"
            },
            {
                "PID": "{datasetPid}"
            },
            {
                "key": "{apiToken}"
            },
            {
                "datasetId": "{datasetId}"
            },
            {
                "localeCode": "{localeCode}"
            }
        ]
    }
}